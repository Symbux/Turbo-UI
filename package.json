  "description": "The UI plugin offers various UI related providers and functionality including a built-in administrator and frontend rendering logic.",
  "main": "lib/index.js",
  "author": "Symbux Dev <hello@symbux.dev> (https://symbux.dev)",
  "license": "Apache-2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/Symbux/Turbo-UI",
  "homepage": "https://github.com/Symbux/Turbo-UI",
  "bugs": {
    "url": "https://github.com/Symbux/Turbo-UI/issues"
  },
  "keywords": [
    "turbo",
    "engine",
    "services",
    "plugin",
    "ui",
    "user-interface",
    "interface",
    "administrator",
    "cms",
    "admin",
    "cms-plugin"
  ],
  "scripts": {
    "start": "ENV=development node -r ts-node/register demo/index.ts",
