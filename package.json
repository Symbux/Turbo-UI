{
  "name": "@symbux/turbo-ui",
  "version": "0.1.0",
  "description": "Turbo UI is a plugin that expands with a full web interface for creating and building applications using the Vue.js 3 framework powered by Vite.",
  "main": "lib/index.js",
  "author": "Symbux Dev <hello@symbux.dev> (https://symbux.dev)",
  "license": "Apache-2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/Symbux/Turbo-Vite",
  "homepage": "https://github.com/Symbux/Turbo-Vite",
  "bugs": {
    "url": "https://github.com/Symbux/Turbo-Vite/issues"
  },
  "keywords": [
    "turbo",
    "engine",
    "services",
    "plugin",
    "ui",
    "user-interface",
    "interface",
    "administrator",
    "cms",
    "admin",
    "cms-plugin"
  ],
  "scripts": {
    "dev": "ENV=development node -r ts-node/register demo/index.ts",
    "dev:prod": "ENV=development VITE_ENV=production node -r ts-node/register demo/index.ts",
    "start": "ENV=production node demo/dist/demo/index.js",
    "build": "rm -rf lib && tsc",
    "build:demo": "rm -rf demo-dist && tsc -p ./tsconfig.demo.json",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf lib && rm -rf ./demo/dist && rm -rf ./web/dist",
    "prepublishOnly": "yarn lint && yarn build",
    "changelog": "auto-changelog --template keepachangelog --commit-limit false --hide-credit",
    "add-peers": "yarn add --peer @symbux/turbo @symbux/injector"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "bulma": "^0.9.4",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "express": "^4.18.1",
    "pinia": "^2.0.14",
    "sass": "^1.53.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-tsc": "^0.38.2"
  },
  "peerDependencies": {
    "@symbux/injector": "^1.4.0",
    "@symbux/turbo": "^0.6.10"
  },
  "dependencies": {
    "vite": "^2.9.13"
  }
}
